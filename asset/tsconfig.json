{
  "include": [
    "../lib/runtime.ts"
  ],
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "removeComments": true,
    "outDir": "../build",
    "skipLibCheck": true,
    "strictNullChecks": false,
    "target": "ES5"
  }
}
